<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.rrr.generated_entities.SprCountryEntity" table="spr_country" schema="public" catalog="airticket">
        <id name="id">
            <column name="id" sql-type="bigint"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)"/>
        </property>
        <property name="shortname">
            <column name="shortname" sql-type="varchar(255)"/>
        </property>
        <property name="flag">
            <column name="flag" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>