<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.rrr.entites.EntitySprCity" table="spr_city" schema="public" catalog="airticket">
        <id name="id">
            <column name="id" sql-type="bigint"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)"/>
        </property>
        <property name="postcode">
            <column name="postcode" sql-type="varchar(255)"/>
        </property>
        <property name="countryId">
            <column name="country_id" sql-type="integer"/>
        </property>
    </class>
</hibernate-mapping>